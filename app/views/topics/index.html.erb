<h1>All topics</h1>
<div class="row">
 <div class="col-sm-1"> <%= link_to 'Create new topic', new_topic_path, class: 'btn btn-primary' %></div> 
</div>

<%= will_paginate %>

<ol>
  <% @topics.each do |q| %>
    <li> <%= q.title %> <%= link_to edit_topic_path(q), class: 'btn' do %>
    <span class="glyphicon glyphicon-edit"></span>
    <% end %></li>
  <% end %>
</ol>

<%= will_paginate %>